# Positive test for phylip_dna_matrin().

# run testprog.exe
$output = `./testprog.exe`;
$status = $?;

# check output
if (($output !~ "FATAL ERROR") && ($output =~ "test passed") && ($status == 0))
{
    print "test passed\n";
}
else
{
    print "test failed\n";
}
